
<div id="g-index">

<% @gigs.each do |gig|%>
<% if gig.user == current_user%>
<p class="pg">**** <%= gig.user.username%> *** <%= gig.venue%> *** <%= gig.date%>  *** <%= gig.time%> *** <%= gig.id%>  <a href="/gigs/<%= gig.id%>">Link</a></p><br>
<%else%>
<p class= "pg">**** <%= gig.user.username%> *** <%= gig.venue%> *** <%= gig.date%>  *** <%= gig.time%> *** <%= gig.id%></p>                 
<% end%>
<%end%>

</div>
